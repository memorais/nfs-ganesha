[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = NFS-Ganesha is an NFSv3,v4,v4.1 fileserver that runs in user mode on most UNIX/Linux systems
[access "refs/*"]
	owner = group user/memorais
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/memorais
	label-Verified = -1..+1 group user/memorais
	push = group user/memorais
	read = group user/memorais
	rebase = group user/memorais
	removeReviewer = group user/memorais
	submit = group user/memorais
[access "refs/heads/*"]
	create = group user/memorais
	push = +force group user/memorais
	pushMerge = group user/memorais
	read = group user/memorais
[access "refs/tags/*"]
	push = group user/memorais
	push = +force group user/memorais
